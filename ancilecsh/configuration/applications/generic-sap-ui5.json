{
	"ApplicationDefinition": {
		"Id": "9000",
		"Description": "Generic Sap UI5 Application",
		"Properties": [{
			"jQuerySelector": "*[id*='sap-ui-bootstrap']"
		}],
		"HelpButton": {
			"Position": "*[class*='sapUiView'] header:visible",
			"Fragment": {
				"XML": "<Button xmlns='sap.m' id='%ID%'></Button>",
				"PlacementMethod": {
					"Name": "addContentRight"
				},
				"SetIconMethod": {
					"Name": "setIcon"
				},
				"SetTooltipMethod": {
					"Name": "setTooltip"
				},
				"AttachPressMethod": {
					"Name": "attachPress"
				}
			}
		}
	}
}