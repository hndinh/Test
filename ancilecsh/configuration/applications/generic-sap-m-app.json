{
	"ApplicationDefinition": {
		"Id": "5000",
		"Description": "New Sap Ui Commons Application",
		"Properties": [
			{
				"jQuerySelector": "*[id*='sap-ui-bootstrap']"
			},
			{
				"jQuerySelector": "*[data-sap-ui-resourceroots*='sap.suite.ui.commons']"
			}
		],
		"HelpButton": {
			"Position": "*[class*='sapUiView']>>header",
			"Fragment": {
				"XML": "<Button xmlns='sap.ui.commons' id='%ID%'></Button>",
				"PlacementMethod": {
					"Name": "addContentRight"
				},
				"SetIconMethod": {
					"Name": "setIcon"
				},
				"SetTooltipMethod": {
					"Name": "setTooltip"
				},
				"AttachPressMethod": {
					"Name": "attachPress"
				}
			}
		}
	}
}
