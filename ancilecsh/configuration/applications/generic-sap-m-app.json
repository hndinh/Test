{
	"ApplicationDefinition": {
		"Id": "2000",
		"Description": "Generic Sap M Application",
		"Properties": [{
			"jQuerySelector": "*[id*='sap-ui-bootstrap']"
		}, {
			"jQuerySelector": "*[data-sap-ui-libs*='sap.m']"
		}],
		"HelpButton": {
			"Position": "*[class*='sapUiView'] header:visible",
			"Fragment": {
				"XML": "<Button xmlns='sap.m' id='%ID%'></Button>",
				"PlacementMethod": {
					"Name": "addContentRight"
				},
				"SetIconMethod": {
					"Name": "setIcon"
				},
				"SetTooltipMethod": {
					"Name": "setTooltip"
				},
				"AttachPressMethod": {
					"Name": "attachPress"
				}
			}
		}
	}
}